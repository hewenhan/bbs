@charset "gb2312";
/** forum::index **/
#wp { padding-top:0;}
.fl { border:none;}
.fl .bm { margin-top:20px;}
.fl .bm_h {background:none;font-size: 20px;font-family: 'Microsoft YaHei';font-weight: normal;height: 30px;line-height: 30px;padding:0px 0 10px 0; border:none;border-bottom: 1px dotted #DEDEDE; margin-bottom:10px;}
.fl .bm_h h1 { font-weight:normal;}
.fl .bm_c { padding:0;}
.fl_g dt { height:30px; line-height:30px; font-size:16px; font-family:'Microsoft YaHei';font-weight: normal;}
.fl_g dl dd { margin-bottom:5px;}
.fl_row td { border: none;}
.fl_tb td, .fl_tb tr { padding:10px; -moz-transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s; -o-transition: all 0.3s ease 0s; transition: all 0.3s ease 0s; -moz-transition: all 0.3s ease 0s;}
.fl_tb h2 { font-size:16px;}
.flg_b1 { background:#f9f9f9;}
.flg_b2 { background:#f1f1f1;}
.fl_tb .flg_b1:hover, .fl_tb .flg_b2:hover { background:#fff;}
.bmw .bm_h a { color: #111;}
/*N-grid*/
.bbs_top { height:270px;}
.bbs_index_thum { width:450px;}
.frame-1-1 { position:relative;}
.frame-1-1-l { width:450px; position:absolute; top:0; left:0; float:none;}
.frame-1-1-r { margin-left:465px; width:auto; float:none;}
.frame-title, .frametitle, .tab-title { background:none;}
.frame-tab .tb { line-height:normal; padding-left:0; height:31px;}
.frame-tab .tb li a { font-weight:normal; font-size:12px; height:31px; line-height:31px; border:none; background:#eee; }
.frame-tab .tb .a a { font-weight:normal;  background:#273138; color:#fff;}
.frame-tab .tb-c { padding:10px 0;}
#chart { margin:0 -10px; width:980px; background:#2F3B43; border-top: 5px solid #161C20 !important; height: 32px; line-height:32px; color: white; margin-bottom:10px;}
#chart .z { padding-left:10px; line-height:30px;}
#chart .xi2 { color:#fff;}
#chart .y { width:350px; background: url({STYLEIMGDIR}/diag-right.png) no-repeat left top ;padding: 5px 10px 10px 10px;}
#anc, #an li { height:16px; line-height:16px;}
#an { margin-bottom:0;}
#an li { width:350px;}
#an li span { width:275px;}
.new-post { display:block; position:absolute;right:0; top:0; padding:0 10px; font-weight: normal; font-size: 12px; height: 31px; line-height: 31px; border: none;background: #195273; color:#fff;}
.new-post:hover { text-decoration:none; background:#273138;}
/** end **/




/** forum::forumdisplay **/
h1.xs2 { font-size:20px !important;}
.boardnav .mn .bml { background-color: {SPECIALBG}; padding:0 10px; border-left: 5px solid #195273;}
#f_pst .bm_c { padding:10px 0;}
#f_pst { margin-top:20px;}
.tl .threadpre .threadpretd {border: none;}
/** end **/




/** forum::index,forum::forumdisplay,home::space,forum::guide **/
.tl th, .tl td { background:#FFF; border-bottom-color:#e6e6e6;  -moz-transition: all 0.3s ease 0s; -webkit-transition: all 0.3s ease 0s; -o-transition: all 0.3s ease 0s; transition: all 0.3s ease 0s; -moz-transition: all 0.3s ease 0s;}
.tl .th th, .tl .th td { background: #F2F2F2}
.bm_c { padding:10px 0;}
/** end **/





/** forum::post **/
#e_menus .p_pop a{ padding:5px 12px;}
/** end **/




/** forum::viewthread,forum::forumdisplay,forum::post,forum::guide,group **/
#newspecial_menu a { padding:10px 0 10px 30px;}
#newspecial_menu li:hover { background-color:#F3F3F3;}
#newspecial_menu a:hover { text-decoration:none;}
#f_pst .plc { padding:0 20px;}
#f_pst .tedt { margin-top:0 !important; border-color: #DEDEDE;}
#visitedforums_menu a { height:auto; width:100%; padding:10px;}
#v_threads li, #v_forums li { padding-left:0; background:none; height:auto;}
#f_pst .plc { padding:0 0 0 10px;}
.tedt .pt { height:125px;}
/** end **/



/** portal::index, portal::list, portal::view**/
#wp { margin: 0 auto; width: 980px; background:#fff; }
#sidebar { width: 300px; float: right; opacity: 0.9; }
#sidebar:hover { opacity: 0.8; }
.post-loop { float: left; width: 100%; position: relative; margin: 0px 0px 20px; padding: 0px 0px 20px; border-bottom: 1px solid rgb(229, 229, 229); }
.post-loop:last-child { margin: 0px; }
.post-thumbnail { display: block; line-height: 0; position: relative; float: left; overflow: hidden; margin: 0px 15px 0px 0px; }
.post-thumbnail img { display: block; position: relative; }
.post-text { float: left; }
.post-tc { overflow:hidden; font-size:13px;}
.post-tc p{ line-height:21px;}
.post-text h2 { font-size: 16px; margin-bottom: 5px; }
.post-scores { float: right; width: 80px; }
.post-scores .site-score, .post-scores .user-score { font-size: 20px; width: 80px; height: 40px; text-align: center; margin: 0px; padding: 15px 0px; }
.post-scores .artcile-view { display:block; background:rgb(229, 167, 0); color:#fff; float:left; font-size: 12px; width: 80px; height: 20px; text-align: center; margin: 0px; padding: 5px 0px; }
.post-meta { float: left; width: 100%; color: #999; font-size: 12px; }
.post-text .post-meta { margin-top: 5px; }
.post-meta a { color: rgb(153, 153, 153); }
.post-meta a:hover { color: rgb(67, 67, 67); }
#dropdown-filter { width: 100%; margin-bottom: 20px; border-bottom: 5px dotted rgb(229, 229, 229); }
.order-by-text { float: left; font-weight: bold; margin-right: 15px; font-size: 14px; margin-bottom: -5px; padding-bottom: 10px; cursor:pointer; }
#dropdown-filter .curr { border-bottom: 5px solid #41A3D2; }
.site-score { float: left; color: rgb(255, 255, 255); padding: 10px 0px; font-size: 35px; margin: 0px 0px 15px; width: 120px; height: 60px; text-align: center; }
.site-score-title { font-size: 12px; }
.user-score { float: left; text-align: center; font-size: 35px; padding: 10px 0px; height: 60px; margin: 0px 20px 0px 0px; color: rgb(255, 255, 255); width: 120px; }
.video-icon { background: url({STYLEIMGDIR}video-icon.png) no-repeat scroll 0% 0% transparent; width: 20px; height: 20px; position: absolute; bottom: 10px; right: 10px; z-index: 2; }
.widget { float: left; width: 100%; margin-bottom: 30px; }
.widget h3 { font-size: 14px; font-weight: bold; padding: 0px 0px 10px; margin-bottom: 20px; border-bottom: 5px solid rgb(229, 229, 229); }
.widget li { list-style-image: none; list-style-type: none; margin: 0px; }
.posts-widget .post-title { font-size:13px; height: 15px; overflow: hidden; display: block; line-height: 15px; }
.posts-widget .post-text .post-scores { width: 34px; margin: 0px 0px 0px 15px; }
.posts-widget .site-score, .posts-widget .user-score { float: left; width: 34px; height: 15px; padding: 7px 0px; font-size: 12px; }
.posts-widget .post-cats { font-size: 12px; margin-top: 5px; color: rgb(102, 102, 102); }
.posts-widget .post-meta { font-size: 12px; margin: 5px 0px 0px; }
.posts-widget .post-meta a { color: rgb(153, 153, 153); }
.posts-widget .post-cats a:hover, .posts-widget .post-meta a:hover { color: rgb(67, 67, 67); }
.posts-widget .post-loop { border:none; padding:0;}
.site-score { background: none repeat scroll 0% 0% rgb(30, 173, 255); }
.user-score { background: none repeat scroll 0% 0% rgb(105, 131, 150); }
.widget a { color: rgb(50, 86, 112); }
.widget a:hover { color: rgb(7, 125, 200); }
.item-avatar a { display: block; margin-bottom: 2px; margin-right: 2px; color: #325670; float: left; }
.posts-widget .post-cats { color: rgb(7, 125, 200); }
.posts-widget .post-cats a { color: rgb(7, 125, 200); }
.posts-widget .post-cats a:hover { color: rgb(50, 86, 112); }
.read-more { color: #077DC8; }

.prediv {clear: both; overflow-y: auto; margin-top:10px; max-height: 600px;}
.prediv img{ max-width: 450px; margin-bottom:10px;}
/** end **/




/** portal::index **/
.frame, .frame-tab { background: none; }
#slider-wrapper { float: left; position: relative; width: 920px; height: 270px; padding: 30px; overflow: hidden; }
.caroufredsel_wrapper { z-index: 2; }
#slider { position: relative; z-index: 2; }
.slide {  width: 160px; height: 220px; line-height: 0; margin: 40px 30px 0px 0px; }
.slide.slide-large { margin: 0px 30px 0px 0px; width: 350px; height: 270px; position:relative; }
#slider-inner-bg { position: absolute; top: 40px; right: 0px; z-index: 1; width: 980px; height: 250px; }
.slide-caption {  margin-top: 10px; }
.slide-caption h2, .slide-caption h2 a { display: block; float: left; color: rgb(255, 255, 255); font-size: 12px; line-height: 19px; text-shadow: 1px 1px 0px rgb(0, 0, 0); margin: 0px; }
.slide-caption.hidden h2 { width: 160px; }
.slide-large .slide-caption { position: absolute; left: 0px; bottom: 0px; height: auto; min-height: 58px; background: url({STYLEIMGDIR}/slide-caption-bg.png) repeat scroll 0% 0% transparent; padding: 20px; width: 310px; }
.slide-large .slide-caption h2, .slide-large .slide-caption h2 a { font-size: 16px; width: 220px; }
.slide-large .post-scores { width: 80px; }
.slide-large .post-scores .site-score, .slide-large .post-scores .user-score { line-height: 40px; }
.slide-small .post-scores { width: 34px; margin: 0px 0px 0px 15px; }
.slide-small .post-scores .site-score, .slide-small .post-scores .user-score { width: 34px; height: 15px; padding: 7px 0px; font-size: 14px; line-height: 14px; }
#slider-nav { float: right; position: absolute; top: 10px; right: 30px; height: 44px; }
#slide-prev { float: left; width: 17px; height: 19px; background: url({STYLEIMGDIR}/sprite.png) repeat scroll 0px 0px transparent; margin-left: 20px; cursor: pointer; }
#slide-prev:hover { background: url({STYLEIMGDIR}/sprite.png) repeat scroll 0px -30px transparent; }
#slide-next { float: left; width: 17px; height: 19px; background: url({STYLEIMGDIR}/sprite.png) repeat scroll -64px 0px transparent; margin-left: 20px; cursor: pointer; }
#slide-next:hover { background: url({STYLEIMGDIR}/sprite.png) repeat scroll -64px -30px transparent; }.
#wp { margin: 0 auto; width: 980px; background:#fff; }
/** end **/




/** portal::list, portal::view**/
#sidebar ul#ds-recent-comments li.ds-comment { font-size:14px; line-height:22px; border-top:none; margin-bottom:10px !important;}
#sidebar ul#ds-recent-comments li.ds-comment .ds-thread-title { line-height:22px; font-size:12px;}
.wp ul#ds-recent-comments li.ds-comment .ds-thread-title a { font-size:12px;}
#sidebar ul#ds-recent-comments li.ds-comment .ds-avatar { top: 10px !important;}
.pg { float: left;}
.pg a, .pgb a, .pg label, .pg strong { margin-right:5px; margin-left:0;}
#wp { margin: 0 auto; width: 980px; background:#fff; }
/** end **/





/** portal::view **/
#nv_portal .vw .d { margin:0; color:#444;}
.vw .d img { max-width: 590px; }
#review-links { float: left; width: 100%; border-bottom: 1px solid #e5e5e5; padding: 0 0 15px 0; margin: 0 0 30px 0; }
#review-links ul { float: left; width: 100%; padding: 0; margin: 0; list-style-image: none; list-style: none; list-style-position: outside; }
#review-links li { float: left; padding: 0 0 0 15px; margin: 0 0 0 15px !important; list-style: none; border-width: 0 0 0 1px; border-style: solid; border-color: #e5e5e5; }
#review-links li:first-child { border: 0; margin: 0 !important; padding: 0; }
#review-links li a { font-weight: bold; padding: 0; }
#review-links li a:hover { text-decoration: underline; }
#review-container { float: left; width: 100%; position: relative; margin: 0 0 20px 0; padding: 0 0 20px 0; border-bottom: 1px solid #e5e5e5; }
#review-left { float: left; margin: 0 30px 0 0; }
#review-left .post-thumbnail { margin: 0 0 10px 0; }
#review-details { float: left; width: 100%; font-size: 11px; line-height: 19px; color: #999; }
#review-right #review-details { margin-top: 0; }
#more-images-link { float: left; width: 100%; font-size: 11px; display: block; line-height: 12px; text-align: center; margin: -5px 0 15px; }
#review-right { float: left; }
#review-panels, .review-panel { float: left; width: 100%; }
.review-panel h2 { font-size: 18px; }
.z .site-score { margin-bottom:0; }
.site-score-title { font-size: 12px; }
.good-bad-panel { float: left; background: #f5f5f5; padding: 15px; font-size: 13px; line-height: 13px; height: 130px;}
.good-point { float: left; height: 25px; width: 100%; padding: 0 0 10px 0; line-height:22px;}
.user-score-title { font-size: 12px; }
.user-score .voted, .user-score .inactive { color: #fff !important; }
#related-items { float: left; width: 100%; margin: 18px 0 12px 0; }
#related-items h3 { display: block; margin: 0 0 20px 0; font-size: 14px; padding: 0 0 15px 0; border-bottom: 5px solid #e5e5e5; }
.related-item { float: left; width: 125px; margin: 0 0 0 30px; }
#related-items div .related-item:first-child { margin-left: 0; }
.related-item .post-thumbnail { margin: 0 0 10px 0; }
.related-item .post-thumbnail img { max-width: 120px; height: auto; }
.related-post-title { font-size: 12px; line-height: 15px; margin: 0; font-weight: bold; }
.related-post-cats, .related-post-cats a { font-size: 11px; color: #999; margin-top: 3px; }
.related-post-cats a:hover { color: #434343; }
.wpfp-span { float: left; display: block; width: 100%; margin: 0 0 10px 0; }
.wpfp-link { float: left; margin-top:5px; line-height: 13px; padding: 5px 10px; background: #2D5069; color: white; font-weight: bold; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; }
.wpfp-link:hover {background: #273138;color: #fff; text-decoration:none;}
#wp { margin: 0 auto; width: 980px; background:#fff; }
.post-meta { margin:5px 0 15px 0; float: none;}
.aricle-ms { overflow:hidden;}
.aricle-zl { margin-left:10px;}
.aricle-ms .aricle-ms-avatar { float:left; margin-right:10px;}
.aricle-ms .aricle-ms-avatar img{ border-radius:5px;}
.wp #ds-thread #ds-reset .ds-post-self { padding-left:0;}
.wp #ds-thread #ds-reset li.ds-post { border-color:#eee;}
.wp #ds-thread #ds-reset .ds-comments, .wp #ds-thread #ds-reset .ds-meta { border:none;}
