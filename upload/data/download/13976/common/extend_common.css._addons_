.footer-widgets-border-left { background: none repeat scroll 0% 0% rgb(45, 49, 52); }
#footer-bottom { background-color: rgb(39, 49, 56); border-top: 10px solid rgb(22, 28, 32); color: rgb(135, 145, 151); }
#footer-bottom a { color: rgb(170, 183, 191); }
